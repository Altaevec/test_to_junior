{
    "name": "test",
    "description": "Test task for the position of junior developer",
    "version": "0.1",
    "manifest_version": 2,

    "browser_action":{
        "default_popup": "index.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },


    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "run_at": "document_end"
    }],

    "permissions": [
        "activeTab"
    ]
}